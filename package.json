{
  "name": "@alwaysai/config-nodejs",
  "description": "A Node.js library for alwaysAI configuration",
  "version": "0.0.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rimraf lib && npm run build",
    "build:watch": "tsc --project tsconfig.json --watch",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:unit && npm run build",
    "test:unit": "jest --coverage src",
    "test:watch": "npm run test:unit -- --watch",
    "preversion": "npm test && npm run build:clean",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@carnesen/coded-error": "0.4.0",
    "@types/mkdirp": "1.0.2",
    "mkdirp": "1.0.4"
  },
  "peerDependencies": {
    "@alwaysai/codecs": "*",
    "fp-ts": ">=2.11.10",
    "io-ts": ">=2.2.16"
  },
  "devDependencies": {
    "@alwaysai/codecs": "0.0.0",
    "@alwaysai/tsconfig": "0.0.1",
    "@alwaysai/tslint-config": "0.0.4",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.25",
    "fp-ts": "2.1.0",
    "io-ts": "2.0.1",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "tempy": "2.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tslint": "5.20.1",
    "typescript": "4.6.3"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "alwaysai",
    "typescript",
    "nodejs"
  ],
  "author": {
    "name": "alwaysAI, Inc.",
    "email": "contact@alwaysai.co"
  },
  "repository": "github:alwaysai/config-nodejs",
  "homepage": "https://github.com/alwaysai/config-nodejs#readme",
  "license": "MIT"
}
